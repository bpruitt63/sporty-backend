{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/teamName.json",
    "type": "array",
    "minItems": 1,
    "maxItems": 30,
    "uniqueItems": true,
    "items": {
        "type": "object",
        "properties": {
            "teamName": {
                "type": "string",
                "minLength": 3,
                "maxLength": 50
            },
            "color": {
                "enum": ["black", 
                        "white", 
                        "gray",
                        "red",
                        "orange",
                        "yellow",
                        "blue",
                        "green",
                        "purple",
                        ""]
            }
        },
        "additionalProperties": false,
        "required": [
            "teamName"
        ]
    }
}